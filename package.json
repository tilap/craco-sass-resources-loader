    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.3",
    "react-scripts": "^2.1.3"
  },
  "peerDependencies": {
    "@craco/craco": "^3.3.1",
    "react-scripts": "^2.1.3"
  },
  "dependencies": {
    "sass-resources-loader": "^2.0.0"
  },
  "husky": {
    "hooks": {
